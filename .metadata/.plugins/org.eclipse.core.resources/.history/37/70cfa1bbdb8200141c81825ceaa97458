
public class Warehouse {
	private Integer data = 0;

	public synchronized Integer get() {
		if(data<=0){
			try {
				wait();
			} catch (InterruptedException e) {}
		}
		data--;
		notify();
		return data;
	}

	public synchronized Integer put() {
		if(data>5){
			try {
				wait();
			} catch (InterruptedException e) {}
		}
		data++;
		notify();
		return data;
	}
}
