import java.util.ArrayList;


public class Butler {
	
	private static Butler b;
	private int[] monksWithFork = new int[5];
	
	public static Butler getInstance(){
		if(b == null){
			b = new Butler();
		}
		return b;
	}
	
	private Butler(){
		for(int i = 0; i<5; i++){
			monksWithFork[i] = 0;
		}
	}
	
	
}
